<schema xmlns="http://www.w3.org/2001/XMLSchema" 
        targetNamespace="http://www.example.org/email" 
        xmlns:tns="http://www.example.org/email"
        xmlns:sf="GenericNS">
        
    <include schemaLocation="emailresponse.xsd"/>
    <!--  import namespace="GenericNS" schemaLocation="../SCA/schemafree.xsd" /-->        

    <complexType name="EmailResponseListType">
        <sequence>
            <element name="jsonemail"  type="tns:EmailResponseType"        minOccurs="0" maxOccurs="unbounded"/>
            <element name="wsemail"    type="tns:EmailResponseType" minOccurs="0"/>
            <element name="localemail" type="tns:EmailResponseType" minOccurs="0"/>
        </sequence>
    </complexType>       
    
</schema>